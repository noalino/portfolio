{
	"extends": ["airbnb", "prettier", "prettier/react"],
	"env": {
		"es6": true,
		"browser": true
	},
	"plugins": ["jsx-a11y", "import", "react"],
	"parser": "babel-eslint",
	"rules": {
		"object-curly-newline": 0,
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [".js", ".jsx"]
			}
		],
		"no-plusplus": 0,
		"no-use-before-define": ["error", { "functions": false }]
	}
}
